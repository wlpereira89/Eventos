@model IEnumerable<Modelo.DAO.evento_composto>

@{
    ViewBag.Title = "Lista";
}

<h2>Exeibição de Eventos</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome);
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => item.Nome);
            </td>
        </tr>
    }

</table>